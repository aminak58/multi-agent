# Test environment configuration
ENVIRONMENT=test

# Service configuration
SERVICE_NAME=orchestrator-test
VERSION=0.1.0

# RabbitMQ (use in-memory for tests)
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USER=test_user
RABBITMQ_PASSWORD=test_password
RABBITMQ_VHOST=/

# Redis (use in-memory for tests)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=test_redis_password
REDIS_DB=1

# Celery
CELERY_BROKER_URL=memory://
CELERY_RESULT_BACKEND=cache+memory://
SIGNAL_QUEUE=signal_queue_test
RISK_QUEUE=risk_queue_test
POSITION_QUEUE=position_queue_test

# MCP Gateway
MCP_GATEWAY_URL=http://localhost:8000

# Monitoring
PROMETHEUS_PORT=9091
LOG_LEVEL=DEBUG
